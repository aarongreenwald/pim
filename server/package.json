{
  "name": "pim-server",
  "version": "1.0.0",
  "description": "pim api service",
  "main": "index.js",
  "scripts": {
    "start": "DOTENV=./.env.dev nodemon -r tsconfig-paths/register src/index.ts ./",
    "build": "tsc && cp ./tsconfig.json ./build/server/tsconfig.json && cp -r ./src/cli/* ./build/server/src/cli",
    "prod": "cd build/server && node -r tsconfig-paths/register src/index.js"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.3",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "pbkdf2-password": "^1.2.1",
    "simple-git": "^2.45.1",
    "sqlite3": "^5.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/node": "^14.11.2",
    "@types/sqlite3": "^3.1.6",
    "chance": "^1.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "repository": {
